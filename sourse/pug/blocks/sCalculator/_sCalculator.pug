mixin sCalculator(h)
	// start sCalculator
	+b.SECTION.sCalculator.section#sCalculator&attributes(attributes)
		+e.container.container
			+b.section-title.text-center
				if h
					h2!= h
				p Заполните один из&nbsp;параметров: тонны, м&sup2;, м.п.&nbsp;и&nbsp;получите сравнительный расчет стоимости шпунта Ларсена и&nbsp;трубошпунта
			+e.content
				+form-wrap
					+e.row.row
						.col
							.form-wrap__input-wrap.form-group
								label
									input.form-wrap__input.form-control(name='number' type='number' placeholder='Введите тонны' step='any')
									div.form-wrap__input-title
										span Введите 
										| тонны
						.col
							.form-wrap__input-wrap.form-group
								label
									input.form-wrap__input.form-control(name='number' type='number' placeholder='Введите м²' step='any')
									div.form-wrap__input-title
										span Введите 
										| м²
						.col
							.form-wrap__input-wrap.form-group
								label
									input.form-wrap__input.form-control(name='number' type='number' placeholder='Введите м.п.' step='any')
									div.form-wrap__input-title
										span Введите 
										| м.п.
				+e.table-wrap
					table
